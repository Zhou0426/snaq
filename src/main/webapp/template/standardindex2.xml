<?xml version="1.0" encoding="UTF-8"?>
<excel id="student" code="student">
    <colgroup>
        <col index="A" width='17em' name="indexSn" type="string"></col>
        <col index="B" width='50em' name="indexName" type="string"></col>
        <col index="C" width='14em' type="enum" format="P,C,D,A" name="pdca"></col>
        <col index="D" width='50em' type="string" name="auditKeyPoints"></col>
        <col index="E" width='15em' type="boolean" format="true,false" name="isKeyIndex"></col>
        <col index="F" width='14em' type="int" format="###" name="integerScore"></col>
        <col index="G" width='17em' name="parentindexSn" type="string"></col>      
    </colgroup>
    <title>
        <tr height="16px">
            <td rowspan="1" colspan="7" value="审核指南导入模板（前五行为标准格式，如行数不够下拉复制即可）"/>
        </tr>
    </title>
    <thead>
        <tr height="16px">
        	<th value="编号" />
            <th value="标准名称" />
            <th value="PDCA阶段" />
            <th value="审核要点" />
            <th value="是否关键指标"/>
            <th value="设计分值"/>
            <th value="父级编号"/>           
        </tr>
    </thead>
    <tbody>
        <tr height="16px" firstrow="2" firstcol="0" repeat="5">
            <td type="string" isnullable="false" name="indexSn"/><!--指标编号 -->
            <td type="string" isnullable="true" name="indexName"/><!--指标名称 -->
            <td type="enum" format="P,D,C,A" isnullable="true" name="pdca"/><!--PCDA阶段 -->
            <td type="string" isnullable="true" name="auditKeyPoints"/><!--审核要点 -->
            <td type="enum" format="是,否" isnullable="true" name="isKeyIndex"/><!--是否关键指标 -->
            <td type="int" format="###" isnullable="true" name="integerScore"/><!--整数分数-->
            <td type="string" isnullable="true" name="parentindexSn"/><!--指标编号 -->
        </tr>
    </tbody>
</excel>